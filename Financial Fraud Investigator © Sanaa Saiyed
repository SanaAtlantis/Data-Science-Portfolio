day': [10, 18, 3, 12, 22],\n","        'merchant_category': ['grocery', 'restaurant', 'travel', 'gas', 'electronics']}\n","transactions_df = pd.DataFrame(data)\n","\n","result_df = detect_fraud(transactions_df)\n","print(result_df)\n"]},{"cell_type":"code","source":["# prompt: write a data analysis programme which can read through financial datasets and use symmetry physics to spot anomalies that do not match the patterns.\n","\n","import pandas as pd\n","from sklearn.preprocessing import StandardScaler\n","from sklearn.decomposition import PCA\n","\n","def detect_financial_anomalies(data, n_components=2):\n","  \"\"\"\n","  Detects anomalies in financial data using symmetry physics principles and PCA.\n","\n","  Args:\n","    data: A Pandas DataFrame containing financial data with relevant features.\n","    n_components: Number of principal components to use for dimensionality reduction.\n","\n","  Returns:\n","    A Pandas DataFrame with an additional column 'anomaly_score' indicating the likelihood of anomaly.\n","  \"\"\"\n","\n","  # Select relevant features (adjust as needed)\n","  features = ['amount', 'time', 'merchant_category_encoded']\n","  X = data[features]\n","\n","  # Preprocess data (scaling is important for PCA)\n","  scaler = StandardScaler()\n","  X_scaled = scaler.fit_transform(X)\n","\n","  # Apply PCA for dimensionality reduction and symmetry analysis\n","  pca = PCA(n_components=n_components)\n","  X_pca = pca.fit_transform(X_scaled)\n","\n","  # Calculate anomaly scores based on distance from the origin in PCA space\n","  data['anomaly_score'] = (X_pca ** 2).sum(axis=1)\n","\n","  return data\n","\n","# Example usage (replace with your actual data)\n","# Assuming 'merchant_category' is already encoded numerically\n","data = {'amount': [100, 25, 5000, 15, 800],\n","        'time': [10, 18, 3, 12, 22],\n","        'merchant_category_encoded': [1, 2, 3, 4, 5]}\n","df = pd.DataFrame(data)\n","\n","result_df = detect_financial_anomalies(df)\n","print(result_df)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"vDHJ_EZR_TES","executionInfo":{"status":"ok","timestamp":1719563887814,"user_tz":-60,"elapsed":319,"user":{"displayName":"Sanaa Saiyed","userId":"12706766209667495205"}},"outputId":"df7ffde7-2912-4689-8f45-5fa0a8ecfa1b"},"execution_count":2,"outputs":[{"output_type":"stream","name":"stdout","text":["   amount  time  merchant_category_encoded  anomaly_score\n","0     100    10                          1       2.517145\n","1      25    18                          2       1.254590\n","2    5000     3                          3       6.195466\n","3      15    12                          4       0.321863\n","4     800    22                          5       3.849931\n"]}]}]}